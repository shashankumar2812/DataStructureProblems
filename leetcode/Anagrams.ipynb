   "source": [
    "s='abc'\n",
    "\n",
    "def perm(s):\n",
    "    if len(s)<=1: return s\n",
    "    perm_list=[]\n",
    "    for c in perm(s[1:]):\n",
    "        for i in range(len(s)):\n",
    "            perm_list.append(c[:i]+s[0:1]+c[i:])\n",
    "    return perm_list\n",
    "        "
   ]
